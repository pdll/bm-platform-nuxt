{
  "name": "bm-nuxt",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "Stepan Yurinov",
  "private": true,
  "scripts": {
    "dev": "nodemon -w server -w nuxt.config.js --exec babel-node server/index.js",
    "build": "nuxt build && babel server --out-dir dist",
    "start": "cross-env NODE_ENV=production nodemon -w dist/index.js --exec node dist/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "babel": {
    "presets": "latest",
    "plugins": "transform-async-to-generator"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-polyfill": "^6.23.0",
    "bcrypt-nodejs": "0.0.3",
    "cross-env": "^3.1.4",
    "koa": "^2.0.0",
    "koa-bodyparser": "^4.1.0",
    "koa-router": "^7.0.1",
    "koa-router-bridge": "^1.0.6",
    "koa-session2": "^2.2.3",
    "moment": "^2.17.1",
    "mysql": "^2.13.0",
    "node-sass": "^4.5.0",
    "numeral": "^1.5.3",
    "nuxt": "latest",
    "postcss-loader": "^1.3.3",
    "postcss-normalize": "^0.3.0",
    "sass-loader": "^6.0.3",
    "sequelize": "^3.30.4",
    "vue-i18n": "^5.0.3",
    "vue-spinner": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-latest": "^6.22.0",
    "css-mqpacker": "^5.0.1",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "koa-passport": "^3.0.0",
    "postcss-aspect-ratio": "^1.0.0",
    "postcss-color-function": "^3.0.0",
    "postcss-css-reset": "^1.0.2",
    "postcss-cssnext": "^2.10.0",
    "postcss-extend": "^1.0.5",
    "postcss-font-awesome": "^0.3.4",
    "postcss-grid": "^2.0.0",
    "postcss-inline-comment": "^3.0.0",
    "postcss-quantity-queries": "^0.4.0",
    "postcss-svg": "^1.0.6",
    "postcss-vertical-rhythm": "^1.1.5",
    "precss": "^1.4.0"
  }
}
